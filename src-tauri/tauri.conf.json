{
  "productName": "notus-app",
  "version": "0.0.1-alpha",
  "identifier": "com.notus.dev",
  "build": {
    "beforeDevCommand": "bun  dev",
    "beforeBuildCommand": "bun run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../build"
  },

  "app": {"windows": [
      {
        "title": "tauri-dev",
        "width": 1280,
        "height": 720,
        "decorations": false
      }
    ],

    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
